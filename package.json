{
  "name": "simplerestclients",
  "version": "0.2.5",
  "description": "A library of components for accessing RESTful services with javascript/typescript.",
  "author": "David de Regt <David.de.Regt@microsoft.com>",
  "module": "dist-es2015/SimpleRestClients.js",
  "main": "dist-cjs/SimpleRestClients.js",
  "scripts": {
    "prepare": "npm run build",
    "clean": "rimraf dist-*",
    "build:types": "tsc -p tsconfig/types.json",
    "build:es2015": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig/cjs.json",
    "build": "run-s clean lint build:*",
    "lint": "tslint --project tsconfig.json -r tslint.json --fix || true",
    "size": "size-limit",
    "test": "npm run clean && karma start --singleRun",
    "test:watch": "npm run clean && karma start"
  },
  "dependencies": {
    "lodash": "^4.17.0",
    "simple-assert-ok": "^0.1.9",
    "synctasks": "^0.3.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.110",
    "@types/faker": "4.1.4",
    "@types/jasmine": "2.8.9",
    "@types/jasmine-ajax": "3.1.38",
    "faker": "4.1.0",
    "fork-ts-checker-webpack-plugin": "0.4.14",
    "jasmine": "3.2.0",
    "jasmine-core": "3.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-ajax": "0.1.13",
    "karma-jasmine-html-reporter": "1.3.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "4.0.0-rc.2",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "size-limit": "0.21.0",
    "ts-loader": "5.3.0",
    "tslint": "5.11.0",
    "tslint-microsoft-contrib": "5.2.1",
    "typescript": "3.1.6",
    "webpack": "4.25.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/SimpleRestClients"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/SimpleRestClients/issues"
  },
  "keywords": [
    "simplerestclient",
    "rest",
    "restclient",
    "xmlhttprequest",
    "superagent"
  ],
  "license": "MIT",
  "types": "dist-types/SimpleRestClients.d.ts",
  "size-limit": [
    {
      "path": "dist-es2015/SimpleRestClients.js",
      "limit": "16 Kb"
    },
    {
      "path": "dist-cjs/SimpleRestClients.js",
      "limit": "16 Kb"
    }
  ]
}
