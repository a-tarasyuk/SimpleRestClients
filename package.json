{
  "name": "simplerestclients",
  "version": "0.2.5",
  "description": "A library of components for accessing RESTful services with javascript/typescript.",
  "author": "David de Regt <David.de.Regt@microsoft.com>",
  "scripts": {
    "prepare": "npm run build",
    "build:types": "tsc --emitDeclarationOnly",
    "build:rollup": "rollup -c",
    "build": "run-s clean type-check tslint build:*",
    "clean": "rimraf dist",
    "tslint": "tslint --project tsconfig.json -r tslint.json --fix || true",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "karma start --browsers Chrome --single-run",
    "test:ci": "karma start --browsers ChromeHeadless --single-run",
    "test:watch": "karma start --browsers Chrome"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "synctasks": "^0.3.3"
  },
  "sideEffects": false,
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/faker": "4.1.4",
    "@types/jasmine": "2.8.9",
    "@types/jasmine-ajax": "3.1.38",
    "@types/lodash": "4.14.117",
    "@types/node": "10.12.0",
    "babel-plugin-lodash": "3.3.4",
    "faker": "4.1.0",
    "jasmine": "3.2.0",
    "jasmine-core": "3.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-ajax": "0.1.13",
    "karma-jasmine-html-reporter": "1.3.1",
    "karma-rollup-preprocessor": "6.1.0",
    "karma-spec-reporter": "0.0.32",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "rollup": "0.66.6",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-filesize": "5.0.1",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-progress": "0.4.0",
    "size-limit": "0.20.1",
    "tslint": "5.11.0",
    "tslint-microsoft-contrib": "5.2.1",
    "typescript": "3.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/SimpleRestClients"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/SimpleRestClients/issues"
  },
  "keywords": [
    "simplerestclient",
    "rest",
    "restclient",
    "xmlhttprequest",
    "superagent"
  ],
  "license": "MIT",
  "module": "dist/SimpleRestClients.esm.js",
  "main": "dist/SimpleRestClients.cjs.js",
  "types": "dist/types/SimpleRestClients.d.ts"
}
